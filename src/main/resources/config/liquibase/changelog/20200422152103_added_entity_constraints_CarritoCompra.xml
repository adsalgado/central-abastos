<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity CarritoCompra.
    -->
    <changeSet id="20200422152103-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cliente_id"
                                 baseTableName="carrito_compra"
                                 constraintName="fk_carrito_compra_cliente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente"/>

        <addForeignKeyConstraint baseColumnNames="producto_id"
                                 baseTableName="carrito_compra"
                                 constraintName="fk_carrito_compra_producto_id"
                                 referencedColumnNames="id"
                                 referencedTableName="producto"/>

    </changeSet>
</databaseChangeLog>
